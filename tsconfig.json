{
  "compilerOptions": {
    "moduleResolution": "node",
    "target": "es2016",
    "module": "commonjs",
    "outDir": "dist", 
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
  },
  "include": ["src/**/*.ts", "tests/**/*.ts", "@types/**/*.d.ts", "src/functions/.spec.ts", "src/assets/*.xlsx"],
  "exclude": ["node_modules", ".vscode", "**/*.spec.ts"]
}
